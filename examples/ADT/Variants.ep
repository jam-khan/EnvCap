@resource module Variants

import TyAliases;

type Day    = Monday 
            | Tuesday 
            | Wednesday 
            | Thursday 
            | Friday 
            | Weekend Int (Int -> String);

let y  = {Weekend 10 (\(x:Int) => { "Whynot" })} as Day;

let res = 
        match y of  
            |   Monday             => { "Monday"    }
            |   Tuesday            => { "Tuesday"   }
            |   Wednesday          => { "Wednesday" }
            |   Thursday           => { "Thursday"  }
            |   Friday             => { "Friday"    }
            |   (Weekend n msg)    => {  msg(1)  }