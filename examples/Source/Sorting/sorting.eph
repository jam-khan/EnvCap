--- sorting header file ---

type Sort = (mergesort: [Int] -> [Int]);

type Util = (
    lessthanEqual:      (Int, [Int]) -> [Int],
    greaterthan:        (Int, [Int]) -> [Int],
    merge:              ([Int], [Int]) -> [Int],
    split:  ((Int, [Int]) -> [Int], Int, [Int]) -> [Int]
);

signature Sorting (
    util: Util
): Sort;
