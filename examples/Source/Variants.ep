type Day    = Monday | Tuesday | Wednesday | Thursday | Friday | Weekend Int (Int -> String);

val res = match ({Weekend 10 (\(x:Int) => { "Hello" })} as Day) of
            case Monday             => { "Monday" }
            case Tuesday            => { "Tuesday" }
            case Wednesday          => { "Wednesday" }
            case Thursday           => { "Thursday" }
            case Friday             => { "Friday" }
            case (Weekend n msg)    => { msg(1) }
